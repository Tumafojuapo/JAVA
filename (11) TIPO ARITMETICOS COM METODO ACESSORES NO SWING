 private void btbDiActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
        int n1 = Integer.parseInt(txtV1.getText());
        int n2 = Integer.parseInt(txtV2.getText()); 
        float divi = n1/n2;
        float d = n1%n2;
        lblDiv.setText(Float.toString(divi));
        lblRest.setText(Float.toString(d));
    }                 
